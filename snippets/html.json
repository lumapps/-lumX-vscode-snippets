{
    /*
        Elements
    */

    // Button
    "{lumX} lx-button": {
        "prefix": "lxbutton",
        "body": [
            "<lx-button lx-color=\"${1:primary}\"",
            "           lx-size=\"${2:m}\"",
            "           lx-type=\"${3:raised}\">",
            "</lx-button>"
        ]
    },

    // Checkbox
    "{lumX} lx-checkbox": {
        "prefix": "lxcheckbox",
        "body": [
            "<lx-checkbox lx-color=\"${1:accent}\"",
            "             lx-theme=\"${2:light}\"",
            "             name=\"${3:checkbox-name}\"",
            "             ng-model=\"$4\">",
            "    <lx-checkbox-label>${5:Checkbox label text}</lx-checkbox-label>",
            "    <lx-checkbox-help>${6:Checkbox help text}</lx-checkbox-help>",
            "</lx-checkbox>"
        ]
    },

    // Data table
    "{lumX} lx-data-table": {
        "prefix": "lxdatatable",
        "body": [
            "<lx-data-table id=\"${1:data-table-id}\"",
            "               lx-border=\"${2:true}\"",
            "               lx-selectable=\"${3:false}\"",
            "               lx-thead=\"${4:vm.dataTableThead}\"",
            "               lx-tbody=\"${5:vm.dataTableTbody}\"",
            "               lx-thumbnail=\"${6:false}\">",
            "</lx-data-table>"
        ]
    },

    // Date picker
    "{lumX} lx-date-picker": {
        "prefix": "lxdatepicker",
        "body": [
            "<lx-date-picker lx-auto-close=\"${1:true}\"",
            "                lx-color=\"${2:primary}\"",
            "                lx-escape-close=\"${3:true}\"",
            "                lx-input-format=\"$4\"",
            "                lx-locale=\"$5\"",
            "                ng-model=\"$6\">",
            "</lx-date-picker>"
        ]
    },

    // Dialog
    "{lumX} lx-dialog": {
        "prefix": "lxdialog",
        "body": [
            "<lx-dialog lx-auto-close=\"${1:true}\"",
            "           lx-escape-close=\"${2:true}\">",
            "    <lx-dialog-header>${3:Dialog header text}</lx-dialog-header>",
            "    <lx-dialog-content>${4:Dialog content text}</lx-dialog-content>",
            "    <lx-dialog-footer>${5:Dialog footer text}</lx-dialog-footer>",
            "</lx-dialog>"
        ]
    },

    // Dialog
    "{lumX} lx-dropdown": {
        "prefix": "lxdropdown",
        "body": [
            "<lx-dropdown lx-depth=\"${1:1}\"",
            "             lx-effect=\"${2:expand}\"",
            "             lx-escape-close=\"${3:true}\"",
            "             lx-hover=\"${4:false}\"",
            "             lx-over-toggle=\"${5:false}\"",
            "             lx-position=\"${6:left}\">",
            "    <lx-dropdown-toggle>",
            "        ${7:<lx-button>Dropdown menu</lx-button>}",
            "    </lx-dropdown-toggle>",
            "    <lx-dropdown-menu>",
            "        ${8:<ul><li><a class=\"dropdown-link\">An action</a></li></ul>}",
            "    </lx-dropdown-menu>",
            "</lx-dropdown>"
        ]
    },

    // Floating Action Button
    "{lumX} lx-fab": {
        "prefix": "lxfab",
        "body": [
            "<lx-fab lx-direction=\"${1:down}\">",
            "    <lx-fab-trigger>",
            "        ${2:<lx-button lx-size=\"xl\" lx-type=\"fab\"><i class=\"mdi mdi-plus\"></i></lx-button>}",
            "    </lx-fab-trigger>",
            "    <lx-fab-actions>",
            "        ${3:<lx-button lx-size=\"l\" lx-type=\"fab\"><i class=\"mdi mdi-delete\"></i></lx-button>}",
            "    </lx-fab-actions>",
            "</lx-fab>"
        ]
    },

    // File input
    "{lumX} lx-file-input": {
        "prefix": "lxfileinput",
        "body": [
            "<lx-file-input lx-label=\"${1:Browse}\"",
            "               lx-accept=\"$2\">",
            "</lx-file-input>"
        ]
    },

    // Icon
    "{lumX} lx-icon": {
        "prefix": "lxicon",
        "body": [
            "<lx-icon lx-id=\"${1:pencil}\"></lx-icon>"
        ]
    },

    // Progress
    "{lumX} lx-progress": {
        "prefix": "lxprogress",
        "body": [
            "<lx-progress lx-color=\"${1:primary}\"",
            "             lx-diameter=\"${2:100}\"",
            "             lx-type=\"${3:circular}\">",
            "</lx-progress>"
        ]
    },

    // Radio button
    "{lumX} lx-radio-button": {
        "prefix": "lxradiobutton",
        "body": [
            "<lx-radio-button lx-color=\"${1:accent}\"",
            "                 name=\"${2:radio-button-name}\"",
            "                 ng-model=\"$3\"",
            "                 value=\"$4\">",
            "    <lx-radio-button-label>${5:Radio button label text}</lx-radio-button-label>",
            "    <lx-radio-button-help>${6:Radio button help text}</lx-radio-button-help>",
            "</lx-radio-button>"
        ]
    },

    // Search filter
    "{lumX} lx-search-filter": {
        "prefix": "lxsearchfilter",
        "body": [
            "<lx-search-filter lx-closed=\"${1:false}\"",
            "                  lx-color=\"${2:black}\"",
            "                  lx-theme=\"${3:light}\"",
            "                  lx-width=\"${4:240}\">",
            "    <input type=\"text\" ng-model=\"$5\">",
            "</lx-search-filter>"
        ]
    },

    // Select
    "{lumX} lx-select": {
        "prefix": "lxselect",
        "body": [
            "<lx-select lx-allow-clear=\"${1:false}\"",
            "           lx-allow-new-value=\"${2:false}\"",
            "           lx-autocomplete=\"${3:false}\"",
            "           lx-choices=\"${4:choices}\"",
            "           lx-choices-custom-style=\"${5:false}\"",
            "           lx-custom-style=\"${6:false}\"",
            "           lx-display-filter=\"${7:false}\"",
            "           lx-error=\"${8:false}\"",
            "           lx-fixed-label=\"${9:false}\"",
            "           lx-helper=\"${10:false}\"",
            "           lx-label=\"${11:Select label text}\"",
            "           lx-loading=\"${12:false}\"",
            "           lx-multiple=\"${13:false}\"",
            "           lx-theme=\"${14:light}\"",
            "           lx-valid=\"${15:false}\"",
            "           lx-view-mode=\"${16:field}\"",
            "           ng-model=\"$17\">",
            "    <lx-select-selected>",
            "        {{ $selected.name }}",
            "    </lx-select-selected>",
            "",
            "    <lx-select-choices>",
            "        {{ $choice.name }}",
            "    </lx-select-choices>",
            "</lx-select>"
        ]
    },

    // Stepper
    "{lumX} lx-stepper": {
        "prefix": "lxstepper",
        "body": [
            "<lx-stepper lx-cancel=\"${1:vm.cancelStepper()}\"",
            "            lx-complete=\"${2:vm.completeStepper()}\"",
            "            lx-is-linear=\"${3:true}\"",
            "            lx-layout=\"${4:horizontal}\"",
            "            lx-show-controls=\"${5:true}\">",
            "    <lx-step lx-is-editable=\"${6:false}\"",
            "             lx-is-optional=\"${7:false}\"",
            "             lx-is-valid=\"${8:true}\"",
            "             lx-label=\"${9:Step 1}\">",
            "        <div flex-container=\"row\" flex-align=\"center center\" class=\"bgc-grey-300\" style=\"height: 200px\">",
            "            <span class=\"fs-title\">${10:Step 1}</span>",
            "        </div>",
            "    </lx-step>",
            "    <lx-step lx-is-editable=\"${11:false}\"",
            "             lx-is-optional=\"${12:false}\"",
            "             lx-label=\"${13:Step 2}\">",
            "        <div flex-container=\"row\" flex-align=\"center center\" class=\"bgc-grey-300\" style=\"height: 200px\">",
            "            <span class=\"fs-title\">${14:Step 2}</span>",
            "        </div>",
            "    </lx-step>",
            "</lx-stepper>"
        ]
    },

    // Switch
    "{lumX} lx-switch": {
        "prefix": "lxswitch",
        "body": [
            "<lx-switch lx-color=\"${1:accent}\"",
            "           name=\"${2:switch-name}\"",
            "           ng-model=\"$3\">",
            "    <lx-switch-label>${4:Switch label text}</lx-switch-label>",
            "    <lx-switch-help>${5:Switch label help text}</lx-switch-help>",
            "</lx-switch>"
        ]
    },

    // Tabs
    "{lumX} lx-tabs": {
        "prefix": "lxtabs",
        "body": [
            "<lx-tabs lx-layout=\"${1:full}\"",
            "         lx-theme=\"${2:light}\"",
            "         lx-color=\"${3:primary}\"",
            "         lx-indicator=\"${4:accent}\"",
            "         lx-active-tab=\"${5:0}\"",
            "         lx-position=\"${6:top}\">",
            "    <lx-tab lx-label=\"${7:First tab label}\">${8:First tab content}</lx-tab>",
            "    <lx-tab lx-label=\"${9:Second tab label}\">${10:Second tab content}</lx-tab>",
            "    <lx-tab lx-label=\"${11:Third tab label}\">${12:Third tab content}</lx-tab>",
            "</lx-tabs>"
        ]
    },

    // Text field
    "{lumX} lx-text-field": {
        "prefix": "lxtextfield",
        "body": [
            "<lx-text-field lx-allow-clear=\"${1:false}\"",
            "               lx-fixed-label=\"${2:false}\"",
            "               lx-focus=\"${3:false}\"",
            "               lx-label=\"${4:Text field label}\"",
            "               lx-theme=\"${5:light}\">",
            "    <input type=\"text\" ng-model=\"$6\">",
            "</lx-text-field>"
        ]
    },

    /*
        Attributes
    */

    // Tooltip
    "{lumX} lx-tooltip": {
        "prefix": "lxtooltip",
        "body": [
            "lx-tooltip=\"${1:Tooltip content}\" lx-tooltip-position=\"${2:top}\" "
        ]
    }

}